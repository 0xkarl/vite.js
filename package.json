{
    "name": "@vite/vitejs",
    "version": "2.1.0",
    "description": "",
    "scripts": {
        "prebuild": "rm -rf ./dist && node build/clean",
        "build:node": "build_target=node webpack",
        "build:web": "build_target=web webpack",
        "build:es5": "mkdir ./src/vitejs/dist && tsc && node build/vitejsES5",
        "build": "yarn run build:es5 && yarn run build:node && yarn run build:web && node build/changePath.js",
        "rpc": "node test/RPC/bin/startRPC",
        "test:rpc": "yarn run prebuild && cross-env NODE_ENV=test nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register test/rpc/index.js",
        "test:coverage": "yarn run prebuild && cross-env NODE_ENV=test nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register test/cover.js  --no-timeouts",
        "test": "yarn run prebuild && cross-env NODE_ENV=test nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register test/index.js  --no-timeouts",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "lint": "eslint --ext js,ts --cache --fix .",
        "lint-all": "eslint --ext js,ts --fix .",
        "publish-alpha": "yarn run build && lerna publish --dist-tag alpha",
        "publish-latest": "yarn run build && lerna publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitelabs/vite.js.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vitelabs/vite.js/issues"
    },
    "homepage": "https://github.com/vitelabs/vite.js#readme",
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.1",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.5",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/register": "^7.0.0",
        "@types/bn.js": "^4.11.3",
        "@types/buffers": "^0.1.31",
        "@types/core-js": "^2.5.0",
        "@types/node": "^10.12.10",
        "@types/mocha": "^5.2.5",
        "babel-eslint": "^10.0.1",
        "babel-plugin-istanbul": "^5.0.1",
        "babelify": "^10.0.0-beta.1",
        "browser-builtins": "^3.3.1",
        "browserify": "~16.2.2",
        "browserify-replace": "^0.9.3",
        "chai": "^4.1.2",
        "core-js": "^2.5.7",
        "coveralls": "^3.0.2",
        "cross-env": "^5.2.0",
        "eslint": "~4.19.1",
        "eslint-config-alloy": "^1.4.2",
        "eslint-plugin-typescript": "^0.14.0",
        "exorcist": "~1.0.1",
        "install": "~0.11.0",
        "jayson": "~2.0.6",
        "mocha": "^5.2.0",
        "nyc": "^13.0.1",
        "pre-commit": "~1.2.2",
        "regenerator-runtime": "^0.12.1",
        "terser-webpack-plugin": "^1.1.0",
        "ts-loader": "^5.3.0",
        "tsconfig-paths": "^3.7.0",
        "tslint": "^5.14.0",
        "typedoc": "^0.13.0",
        "typedoc-webpack-plugin": "^1.1.4",
        "typescript-eslint-parser": "^22.0.0",
        "vinyl-source-stream": "~2.0.0",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "@sisi/ed25519-blake2b-hd-key": "^1.0.2",
        "@sisi/tweetnacl-blake2b": "^1.0.0",
        "babel-loader": "^8.0.4",
        "bip39": "^2.5.0",
        "blake2b": "^2.1.3",
        "blakejs": "^1.1.0",
        "bn.js": "^4.11.8",
        "browserify-aes": "^1.2.0",
        "buffer": "~5.2.1",
        "create-hmac": "^1.1.7",
        "es6-promise": "^4.2.5",
        "jsonrpc-lite": "^2.0.3",
        "lerna": "^3.13.1",
        "net": "^1.0.2",
        "pure-uuid": "^1.5.3",
        "qs": "^6.6.0",
        "scryptsy": "^2.0.0",
        "ts-node": "^7.0.1",
        "websocket": "^1.0.26",
        "xhr2": "^0.1.4"
    },
    "nyc": {
        "require": [
            "ts-node/register",
            "@babel/register"
        ],
        "extension": [
            ".js",
            ".ts"
        ],
        "include": [
            "src/*"
        ],
        "exclude": [
            "*.d.ts"
        ],
        "reporter": [
            "html",
            "text"
        ],
        "sourceMap": false,
        "instrument": true
    }
}
