{
    "name": "@vite/vitejs",
    "private": true,
    "description": "Vite Javascript API",
    "scripts": {
        "format-type": "node build/type",
        "prebuild": "rm -rf ./dist && rm -rf ./distSrc && node build/clean",
        "build:node": "build_target=node webpack",
        "build:web": "build_target=web webpack",
        "build:vitejsES5": "tsc && node build/vitejsES5",
        "build:vitets": "cp -r ./src/. ./distSrc && cp -r ./distSrc ./src/vitejs/distSrc",
        "build:vitejs": "yarn run build:vitets && yarn run build:vitejsES5",
        "build:all": "yarn run build:node && yarn run build:web",
        "build": "yarn run build:vitejs && yarn run build:all && node build/writePkgJson.js",
        "test:coverage": "nyc npm run test",
        "test": "yarn run format-type && cross-env NODE_ENV=test mocha --reporter=progress --require ts-node/register -r tsconfig-paths/register test/index.js  --no-timeouts",
        "coverage": "npm run test:coverage && nyc report --reporter=lcov",
        "lint": "eslint --ext js,ts --cache .",
        "lint:fix": "eslint --ext js,ts --fix --cache .",
        "lint-all": "eslint --ext js,ts .",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
        "release": "npm test && npm run build && node build/release.js",
        "commit": "git-cz"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitelabs/vite.js.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vitelabs/vite.js/issues"
    },
    "homepage": "https://github.com/vitelabs/vite.js#readme",
    "devDependencies": {
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.15",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/register": "^7.13.14",
        "@types/bn.js": "^5.1.0",
        "@types/buffers": "^0.1.31",
        "@types/core-js": "^2.5.4",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@vite/vitejs": "^2.3.12",
        "@vite/vitejs-ws": "^2.3.12",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-istanbul": "^6.0.0",
        "babelify": "^10.0.0",
        "browser-builtins": "^3.3.1",
        "browserify": "^17.0.0",
        "browserify-replace": "^1.1.0",
        "chai": "^4.3.4",
        "commitizen": "^4.2.3",
        "core-js": "^3.10.1",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.24.0",
        "eslint-config-alloy": "^3.10.0",
        "lerna": "^4.0.0",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0",
        "pre-commit": "^1.2.2",
        "regenerator-runtime": "^0.13.7",
        "tern": "^0.24.3",
        "terser-webpack-plugin": "^5.1.1",
        "ts-loader": "^8.1.0",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typedoc": "^0.20.35",
        "typedoc-webpack-plugin": "^1.1.4",
        "typescript": "^4.2.4",
        "webpack": "^5.32.0",
        "webpack-cli": "^4.6.0",
        "webpack-merge": "^5.7.3",
        "yarn-upgrade-all": "^0.5.4"
    },
    "dependencies": {
        "@sisi/ed25519-blake2b-hd-key": "^1.0.2",
        "@sisi/tweetnacl-blake2b": "^1.0.0",
        "bip39": "^3.0.3",
        "blake2b": "^2.1.3",
        "blakejs": "^1.1.0",
        "bn.js": "^5.2.0",
        "browserify-aes": "^1.2.0",
        "buffer": "^6.0.3",
        "conventional-changelog-cli": "^2.1.1",
        "create-hmac": "^1.1.7",
        "es6-promise": "^4.2.8",
        "jsonrpc-lite": "^2.2.0",
        "net": "^1.0.2",
        "pure-uuid": "^1.6.2",
        "qs": "^6.10.1",
        "scryptsy": "^2.1.0",
        "websocket": "^1.0.33",
        "xhr2": "^0.2.1"
    },
    "nyc": {
        "extension": [
            ".js",
            ".ts"
        ],
        "include": [
            "src/*"
        ],
        "exclude": [
            "*.d.ts",
            "src/*/dist/**",
            "src/*/es5/**"
        ],
        "reporter": [
            "html",
            "text"
        ],
        "sourceMap": false,
        "instrument": true
    }
}
