{
    "name": "@vite/vitejs",
    "version": "1.1.0-1",
    "description": "",
    "scripts": {
        "postinstall": "lerna bootstrap",
        "build": "yarn run postinstall && lerna run build",
        "rpc": "node test/bin/startRPC",
        "test:rpc": "cross-env NODE_ENV=test nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register test/RPC/index.js --no-timeouts",
        "test": "yarn run postinstall && cross-env NODE_ENV=test nyc mocha --compilers ts:ts-node/register -r tsconfig-paths/register test/index.js  --no-timeouts",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "lint": "eslint ./ --cache",
        "lint:all": "eslint ./",
        "prerelease-alpha": "npm run build && npm version prerelease",
        "release-alpha": "npm publish --access-public --tag alpha",
        "release": ""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitelabs/vite.js.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vitelabs/vite.js/issues"
    },
    "homepage": "https://github.com/vitelabs/vite.js#readme",
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.1",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.1.5",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/register": "^7.0.0",
        "@types/bn.js": "^4.11.3",
        "@types/buffers": "^0.1.31",
        "@types/core-js": "^2.5.0",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.10",
        "babel-loader": "^8.0.4",
        "babel-plugin-istanbul": "^5.0.1",
        "browser-builtins": "^3.3.1",
        "chai": "^4.1.2",
        "core-js": "^2.5.7",
        "coveralls": "^3.0.2",
        "cross-env": "^5.2.0",
        "eslint": "~4.19.1",
        "exorcist": "~1.0.1",
        "install": "~0.11.0",
        "jayson": "~2.0.6",
        "lerna": "^3.8.4",
        "mocha": "^5.2.0",
        "nyc": "^13.0.1",
        "pre-commit": "~1.2.2",
        "regenerator-runtime": "^0.12.1",
        "terser-webpack-plugin": "^1.1.0",
        "ts-loader": "^5.3.0",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.7.0",
        "typedoc": "^0.13.0",
        "typedoc-webpack-plugin": "^1.1.4",
        "vinyl-source-stream": "~2.0.0",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "bip39": "^2.5.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-node": "^8.0.0",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0"
    },
    "nyc": {
        "require": [
            "@babel/register"
        ],
        "reporter": [
            "html",
            "text"
        ],
        "sourceMap": false,
        "instrument": false
    }
}
