{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "baseUrl": "./src",
        "paths": {
            "~@vite/vitejs-abi": ["abi/index.ts"],
            "~@vite/vitejs-addraccount": ["addrAccount/index.ts"],
            "~@vite/vitejs-account": ["account/index.ts"],
            "~@vite/vitejs-accountblock": ["accountBlock/index.ts"],
            "~@vite/vitejs-client": ["client/index.ts"],
            "~@vite/vitejs-communication/*": ["communication/*"],
            "~@vite/vitejs-constant": ["constant/index.ts"],
            "~@vite/vitejs-error": ["error/index.ts"],
            "~@vite/vitejs-hdaccount": ["hdAccount/index.ts"],
            "~@vite/vitejs-hdaddr": ["hdAddr/index.ts"],
            "~@vite/vitejs-keystore": ["keystore/index.ts"],
            "~@vite/vitejs-netprocessor": ["netProcessor/index.ts"],
            "~@vite/vitejs-privtoaddr": ["privToAddr/index.ts"],
            "~@vite/vitejs-utils": ["utils/index.ts"],
            "~@vite/vitejs-ws": ["WS/index.js"],
            "~@vite/vitejs-http": ["HTTP/index.js"],
            "~@vite/vitejs-ipc": ["IPC/index.js"],
            "~@vite/vitejs": ["vitejs/index.ts"],
            "~@vite/vitejs-accountblock/builtin": ["accountBlock/builtin.ts"]
        },
        "lib": [
            "es5",
            "es2017",
            "es2015",
            "es2017.typedarrays"
        ],
        "outDir": "./packages/vitejs/es5"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}